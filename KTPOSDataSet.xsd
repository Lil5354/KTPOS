                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT 
    F.NAME AS FoodName,
    BI.COUNT AS Quantity
FROM 
    FOOD F
    INNER JOIN (
        SELECT IDFD, COUNT 
        FROM BILLINF 
        WHERE (@IDBILL IS NULL OR IDBILL = @IDBILL)
        AND (@IDFD IS NULL OR IDFD = @IDFD)
    ) BI ON F.ID = BI.IDFD
ORDER BY 
    F.NAME;

-- Parameters
DECLARE @IDBILL INT = NULL
DECLARE @IDFD INT = NULL</CommandText>
